{
  "name": "hacker-news-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^3.2.3",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.9",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-loadable": "^5.4.2",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/recompose": "^0.30.0",
    "@types/reduce-reducers": "^0.1.3",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-thunk": "^2.1.0",
    "@types/reselect": "^2.2.0",
    "axios": "^0.18.0",
    "bson": "^4.0.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "node-sass": "^4.10.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "recompose": "^0.30.0",
    "reduce-reducers": "^0.4.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "timeago.js": "^4.0.0-beta.2",
    "typescript": "^3.1.6",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/ -o src/ --recursive",
    "css-watch": "npm run css-build -- --watch",
    "app": "REACT_APP_ENV=development concurrently \"npm run server\" \"craco start --verbose\" \"npm run css-watch\"",
    "start": "REACT_APP_ENV=development concurrently \"craco start --verbose\" \"npm run css-watch\"",
    "start:staging": "REACT_APP_ENV=staging craco start --verbose",
    "start:production": "REACT_APP_ENV=production craco start",
    "build": "craco build && mv ./build ./docs",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier -w ./src/**/*.tsx?",
    "analyze": "npm run build -- --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:8000",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.1.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.2",
    "redux-devtools": "^3.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0"
  }
}
